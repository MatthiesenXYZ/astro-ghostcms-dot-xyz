---
import Markdown from '../RemoteMarkdown.astro';
import CodeSpan from '../CodeSpan.astro';
import CodeBlock from '../CodeBlock.astro';
import Heading from '../Heading.astro';
import Note from '../Note.astro';

const readme = await fetch('https://raw.githubusercontent.com/MatthiesenXYZ/astro-ghostcms/main/packages/create-astro-ghostcms/CHANGELOG.md').then((res)=>res.text());
---
<Markdown
    content={readme}
    components={{ Heading, CodeBlock, CodeSpan, Note }}
    />